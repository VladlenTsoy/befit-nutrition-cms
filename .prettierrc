{
  "printWidth": 120,
  "tabWidth": 4,
  "semi": false,
  "bracketSpacing": false,
  "trailingComma": "none",
  "arrowParens": "always",
  "useTabs": false,
  "parser": "typescript",
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "parser": "json"
      }
    }
  ]
}
